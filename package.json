{
	"name": "nextjs-starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build && npm run build:storybook",
		"build:next": "next build",
		"build:storybook": "storybook build -o public/storybook",
		"start": "next start -p ${PORT:=3000}",
		"lint": "run-p lint:*",
		"lint:next": "next lint",
		"lint:js": "eslint \"*/**/*.{js,jsx,ts,tsx}\" --fix",
		"lint:css": "stylelint \"./**/*.{css,scss}\" --fix",
		"lint:ts": "tsc -p tsconfig.json --noEmit",
		"format": "prettier \"*/**/*.{js,jsx,ts,tsx,json,md,css,scss}\" --write",
		"formatw": "prettier \"{,!(node_modules)/**/}*.js\"",
		"storybook": "storybook dev -p 6006",
		"prepare": "husky install",
		"test": "vitest",
		"test:ci": "vitest run",
		"new": "cross-env HYGEN_TMPLS=generators hygen new"
	},
	"dependencies": {
		"clsx": "^2.0.0",
		"next": "^14.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@axe-core/react": "^4.8.1",
		"@commitlint/cli": "^18.2.0",
		"@commitlint/config-conventional": "^18.1.0",
		"@storybook/addon-a11y": "^7.5.3",
		"@storybook/addon-essentials": "^7.5.3",
		"@storybook/addon-interactions": "^7.5.3",
		"@storybook/addon-links": "^7.5.3",
		"@storybook/blocks": "^7.5.3",
		"@storybook/nextjs": "^7.5.3",
		"@storybook/testing-library": "^0.2.2",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.5.1",
		"@types/jest-axe": "^3.5.7",
		"@types/node": "^20.8.10",
		"@types/react": "^18.2.36",
		"@types/react-dom": "^18.2.14",
		"@typescript-eslint/eslint-plugin": "^6.9.1",
		"@typescript-eslint/parser": "^6.9.1",
		"@vitejs/plugin-react": "^4.1.1",
		"@vitest/coverage-v8": "^0.34.6",
		"cross-env": "^7.0.3",
		"eslint": "^8.53.0",
		"eslint-config-next": "^14.0.1",
		"eslint-plugin-deprecation": "^2.0.0",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-storybook": "^0.6.15",
		"husky": "^8.0.3",
		"hygen": "^6.2.11",
		"jsdom": "^22.1.0",
		"lint-staged": "^15.0.2",
		"msw": "^1.3.2",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.31",
		"postcss-loader": "^7.3.3",
		"postcss-preset-env": "^9.3.0",
		"postcss-safe-parser": "4.0.2",
		"prettier": "^3.0.3",
		"storybook": "^7.5.3",
		"storybook-addon-performance": "^0.17.1",
		"storybook-css-modules": "^1.0.8",
		"stylelint": "^15.11.0",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-order": "^6.0.3",
		"typescript": "^5.2.2",
		"vite-tsconfig-paths": "^4.2.1",
		"vitest": "^0.34.6",
		"vitest-axe": "^0.1.0"
	},
	"peerDependencies": {
		"postcss": "^8.4.18"
	},
	"msw": {
		"workerDirectory": "public"
	},
	"engines": {
		"node": ">=18 <=20"
	}
}
