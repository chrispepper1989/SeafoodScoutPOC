{
	"name": "nextjs-starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build:next": "next build",
		"build": "next build && npm run build-storybook",
		"start": "next start -p ${PORT:=3000}",
		"lint": "run-p lint:*",
		"lint:next": "next lint",
		"lint:js": "eslint \"*/**/*.{js,jsx,ts,tsx}\" --fix",
		"lint:css": "stylelint \"./**/*.{css,scss}\" --fix",
		"lint:ts": "tsc -p tsconfig.json --noEmit",
		"format": "prettier \"*/**/*.{js,jsx,ts,tsx,json,md,css,scss}\" --write",
		"formatw": "prettier \"{,!(node_modules)/**/}*.js\"",
		"storybook": "start-storybook -s public -p 6006",
		"build-storybook": "build-storybook -o public/storybook",
		"prepare": "husky install",
		"test": "vitest",
		"test:ci": "vitest run",
		"new": "cross-env HYGEN_TMPLS=generators hygen new"
	},
	"dependencies": {
		"clsx": "^1.2.1",
		"next": "^13.2.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@axe-core/react": "^4.6.0",
		"@commitlint/cli": "^17.4.4",
		"@commitlint/config-conventional": "^17.4.4",
		"@storybook/addon-a11y": "^6.5.16",
		"@storybook/addon-actions": "^6.5.16",
		"@storybook/addon-essentials": "^6.5.16",
		"@storybook/addon-interactions": "^6.5.16",
		"@storybook/addon-links": "^6.5.16",
		"@storybook/addon-postcss": "^2.0.0",
		"@storybook/builder-webpack5": "^6.5.16",
		"@storybook/manager-webpack5": "^6.5.16",
		"@storybook/react": "^6.5.16",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest-axe": "^3.5.5",
		"@types/node": "^18.15.0",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.54.1",
		"@typescript-eslint/parser": "^5.54.1",
		"@vitejs/plugin-react": "^3.1.0",
		"cross-env": "^7.0.3",
		"eslint": "^8.35.0",
		"eslint-config-next": "^13.2.4",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"hygen": "^6.2.11",
		"jsdom": "^21.1.0",
		"lint-staged": "^13.1.2",
		"msw": "^1.1.0",
		"npm-run-all": "^4.1.5",
		"postcss-loader": "^7.0.2",
		"postcss-preset-env": "^8.0.1",
		"postcss-safe-parser": "4.0.2",
		"prettier": "^2.8.4",
		"storybook-addon-performance": "^0.17.1",
		"stylelint": "^14.16.1",
		"typescript": "^4.9.5",
		"vitest": "^0.29.3",
		"vitest-axe": "^0.1.0"
	},
	"peerDependencies": {
		"postcss": "^8.4.18"
	},
	"msw": {
		"workerDirectory": "public"
	},
	"engines": {
		"node": "~18.12.0"
	}
}
